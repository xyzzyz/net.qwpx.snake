<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.qwpx.snake"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:targetSdkVersion="10" android:minSdkVersion="10"></uses-sdk>


    <application android:icon="@drawable/icon" android:label="@string/app_name"
    android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:name=".QWPXSnake"
                  android:label="@string/app_name" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="GameActivity" android:screenOrientation="landscape"></activity>
        <activity android:screenOrientation="landscape" android:name="EnterHighScoreNameActivity"></activity>
        <activity android:name="HighScoreActivity" android:screenOrientation="landscape"></activity>

    </application>
</manifest>